def setZeros(m):
    row = [0] * len(m)
    column = [0] * len(m[0])
    for i in range(0, len(m)):
        for j in range(0, len(m[0])):
            if m[i][j] == 0:
                row[i] = 1
                column[j] = 1
    for i in range(0, len(m)):
        for j in range(0, len(m[0])):
            if (row[i] == 1 or column[j] == 1):
                m[i][j] = 0
